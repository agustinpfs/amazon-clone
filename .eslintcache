ged((authUser) => {\n      console.log(\"THE USER IS >>> \", authUser);\n\n      if (authUser) {\n        // the user just logged in / the user was logged in\n\n        dispatch({\n          type: \"SET_USER\",\n          user: authUser,\n        });\n      } else {\n        // the user is logged out\n        dispatch({\n          type: \"SET_USER\",\n          user: null,\n        });\n      }\n    });\n  }, []);\n\n\n  return (\n    <Router>\n      <div className=\"app\">\n\n        <Switch>\n\n          <Route path='/orders'>\n            <Header />\n            <Orders />\n          </Route>\n         \n          <Route path='/login'>\n            <Login />\n          </Route>\n          \n          <Route path='/checkout'>\n            <Header />\n            <Checkout />\n          </Route>\n         \n          <Route path='/payment'>\n            <Header />\n            <Elements stripe={promise}>\n              <Payment />\n            </Elements>\n          </Route>\n          \n          <Route path='/'>\n            <Header />\n            <Home />\n          </Route>\n\n        </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/agujex/agujex_HOME/knowledge/REACT/amazon-clone/src/reportWebVitals.js",[],"/Users/agujex/agujex_HOME/knowledge/REACT/amazon-clone/src/Header.js",["81"],"/Users/agujex/agujex_HOME/knowledge/REACT/amazon-clone/src/Home.js",[],"/Users/agujex/agujex_HOME/knowledge/REACT/amazon-clone/src/Product.js",[],"/Users/agujex/agujex_HOME/knowledge/REACT/amazon-clone/src/Checkout.js",["82"],"import React from 'react';\nimport './Checkout.css';\nimport Subtotal from './Subtotal';\nimport { useStateValue } from \"./StateProvider\";\nimport CheckoutProduct from \"./CheckoutProduct\";\n\n\nfunction Checkout() {\n    const [{ basket, user }, dispatch] = useStateValue();\n\n\n    return (\n        <div className='checkout'>\n            <div className='checkout__left'>\n                <img \n                    classN